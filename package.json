{
  "name": "boilerplate-react-apollo",
  "version": "1.0.0",
  "description": "A PostGraphile boilerplate using React and Apollo on the client side.",
  "main": "index.js",
  "scripts": {
    "db:migrate:create": "node db/scripts/create-migration.js",
    "db:migrate:dev": ". ./scripts/source-env && npm run db:migrate:_cmd up && ./scripts/dump-schema",
    "db:mrm:dev": ". ./scripts/source-env && npm run db:migrate:_cmd up && npm run db:migrate:_cmd down && npm run db:migrate:_cmd up && ./scripts/dump-schema",
    "db:rollback:dev": ". ./scripts/source-env && npm run db:migrate:_cmd down && ./scripts/dump-schema",
    "db:reset:dev": ". ./scripts/source-env && npm run db:migrate:_cmd reset && npm run db:migrate:_cmd up && ./scripts/dump-schema",
    "db:migrate:_cmd": "db-migrate --migrations-dir db/migrations/.db-migrate",
    "lint": "eslint .",
    "setup": "scripts/setup",
    "server:dev": "cd server && nodemon",
    "client:dev": "cd client && BROWSER=none PORT=\"$CLIENT_PORT\" npm run start",
    "start": ". ./scripts/source-env && concurrently --kill-others --names server,client 'npm run server:dev' 'npm run client:dev'",
    "worker": ". ./scripts/source-env && graphile-worker",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/boilerplate-react-apollo.git"
  },
  "keywords": [
    "postgraphile",
    "graphile",
    "boilerplate",
    "react",
    "apollo",
    "ssr",
    "graphql",
    "postgresql",
    "pg",
    "postgres"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/boilerplate-react-apollo/issues"
  },
  "homepage": "https://github.com/graphile/boilerplate-react-apollo#readme",
